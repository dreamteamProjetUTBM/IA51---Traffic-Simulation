package environments

import org.newdawn.slick.geom.Vector2f
import java.util.UUID

/** 
 * @author jerem
 * 
 */
abstract class Vehicle extends EnvironmentObject {
	
	val agentId : UUID
	
	
	new(vector : Vector2f, id : UUID){
		super(vector,true,true)
		agentId = id
	}

	final def getAgentId : UUID {
		this.agentId
	}

	abstract def moveVehicle(newPos : Vector2f);
	
}
