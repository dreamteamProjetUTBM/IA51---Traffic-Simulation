package environments

import java.util.ArrayList
import org.newdawn.slick.geom.Vector2f

/** 
 * @author jerem
 * 
 */
abstract class EnvironmentObject extends Entity implements EntityDrawable, EntityUpdateable, Runnable {
	
	var points = new ArrayList<Point>
	
	new()
	{
		var vec = new Vector2f(0.0f, 0.0f)
		super(vec)
	}
}
